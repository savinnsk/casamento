

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
    id String @id @default(uuid())
    name String
    phone String
    isConfirmed Boolean

    @@map("users")
}

model Gift {
    id String @id @default(uuid())
    name String
    userId String?
    description String?
    isAvaliable Boolean?
    
    @@map("gifts")
}


